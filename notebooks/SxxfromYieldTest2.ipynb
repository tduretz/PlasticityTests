     "text": [
      "σxx = -σyy + 2.0 * σyy ./ cos(ϕ) .^ 2 + 2.0 * sqrt(σxy .^ 2 .* sin(ϕ) .^ 2 - σxy .^ 2 + σyy .^ 2 .* sin(ϕ) .^ 2) ./ cos(ϕ) .^ 2\n"
     ]
    }
   ],
   "source": [
    "ϕ, σxx, σyy, σxy = symbols('ϕ, σxx, σyy, σxy')\n",
    "τ_i    = sqrt(0.25*(σxx-σyy)**2 + σxy**2)\n",
    "f      = τ_i + 0.5*(σxx+σyy)*sin(ϕ)\n",
    "print('σxx = ' + julia_code(solve(f, σxx)[1]))\n"
   ]
  },
